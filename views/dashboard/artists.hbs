<h1>Here are the artists</h1>

<div>
    {{#each artists}}
    <p>{{this.name}}</p>
    <p>{{this.isBand}}</p>
    <p>{{this.description}}</p>
    <img src="{{this.picture}}" alt="image">
    <form method="post" action="/dashboard/artist/delete/{{this._id}}">
        <button>Delete</button>
</form>
    {{/each}}
</div>